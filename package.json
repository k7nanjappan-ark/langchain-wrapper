{
  "name": "@slauth.io/langchain-wrapper",
  "version": "1.4.0",
  "description": "Reusable wrapper of langchain",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slauth-io/langchain-wrapper.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --removeComments --declaration",
    "prepare": "husky install",
    "lint": "eslint ./src --ext ts,tsx",
    "lint-fix": "eslint ./src --ext ts,tsx --fix --cache",
    "format": "prettier ./src --write",
    "format-check": "prettier ./src --check",
    "prepack": "npm run build",
    "postversion": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "semantic-release": "^22.0.8",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "langchain": "^0.0.186",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.21.4"
  }
}
